digraph ProgramStructureGraph {
  rankdir=TB;
  compound=true;
  
  // Node styles
  node [shape=box];
  
  subgraph cluster_call_graph {
    label="Call Graph";
    style=filled;
    fillcolor=lightgrey;
    
    func1 [label="main\nmain.c:10", shape=doubleoctagon, fillcolor=green, style=filled];
    func2 [label="compute\ncompute.c:5", shape=box, fillcolor=lightblue, style=filled];
    func3 [label="matrix_multiply\nmatrix.c:15", shape=box, fillcolor=lightblue, style=filled];
    func4 [label="vector_add\nvector.c:8", shape=box, fillcolor=lightblue, style=filled];
    func5 [label="io_read\nio.c:20", shape=box, fillcolor=lightblue, style=filled];
    func6 [label="helper\nutils.c:12", shape=box, fillcolor=lightblue, style=filled];
    
    // Call edges
    func1 -> func2;
    func1 -> func5;
    func2 -> func3;
    func2 -> func4;
    func2 -> func6;
  }
  
  subgraph cluster_loops {
    label="Loops";
    style=filled;
    fillcolor=lightyellow;
    
    loop10 [label="outer_loop\nType: for\nDepth: 1", shape=ellipse, fillcolor=yellow, style=filled];
    loop11 [label="inner_loop\nType: for\nDepth: 2", shape=ellipse, fillcolor=yellow, style=filled];
  }
}
